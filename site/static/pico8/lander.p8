pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
lander_x = 10
lander_y = 30
v_x = 0
v_y = -2
size=10

function _update()
  cls()
  move_l()
  update_vel()
  draw_lander()
end

function update_vel()
  v_y += 0.1
  
  if (btn(0)) then v_y=v_y-.2 end
if (btn(1)) then v_x=v_x-.2 end

  print(v_y.."\n"..lander_y, 90,10)
end

function move_l()
  lander_x += v_x
  lander_y += v_y
end

function draw_lander()
 rectfill(
   lander_x,
   lander_y,
   lander_x+size,
   lander_y+size,
   12)
 

end
